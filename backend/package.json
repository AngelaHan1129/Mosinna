{
  "name": "express-api-starter",
  "version": "1.2.0",
  "description": "A basic starter for an express.js API",
  "main": "index.js",
  "scripts": {
    "start": "node src/index.js",
    "server:dev": "nodemon src/index.js --exec babel-node",
    "dev": "npm run migrate:dev && npm run server:dev",
    "lint": "eslint --fix src",
    "test": "jest",
    "test:e2e": "npm run migrate:test && npm run test",
    "migrate:dev": "sequelize-cli db:migrate --config src/db/config/config.json --env development",
    "migrate:test": "sequelize-cli db:migrate --config src/db/config/config.json --env test"
  },
  "keywords": [],
  "author": "CJ R. <cj@null.computer> (https://w3cj.now.sh)",
  "repository": {
    "type": "git",
    "url": "https://github.com/w3cj/express-api-starter.git"
  },
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "express-api-starter": "file:",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "pg": "^8.13.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.25.7",
    "@babel/core": "^7.25.8",
    "@babel/node": "^7.25.7",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/preset-env": "^7.25.8",
    "@types/express": "^5.0.0",
    "babel-jest": "^29.7.0",
    "babel-node": "^0.0.1-security",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.30.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.5",
    "sequelize-auto": "^0.8.8",
    "sequelize-cli": "^6.6.2",
    "supertest": "^7.0.0"
  }
}
